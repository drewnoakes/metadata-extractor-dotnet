{
  "id": "MetadataExtractor-Signed",
  "title": "Metadata Extractor",
  "version": "2.0.0-*",
  "authors": [ "Drew Noakes" ],
  "copyright": "Copyright Drew Noakes 2002-2017",
  "description": "MetadataExtractor is a straightforward and lightweight library for reading metadata from image and video files. It reads Exif, IPTC, XMP, ICC, Photoshop, WebP, PNG, BMP, GIF, ICO, PCX metadata from JPEG, TIFF WebP, PSD, PNG, BMP, GIF, ICO, PCX and camera RAW files. Camera manufacturer specific support exists for Agfa, Canon, Casio, Epson, Fujifilm, Kodak, Kyocera, Leica, Minolta, Nikon, Olympus, Panasonic, Pentax, Reconyx, Sanyo, Sigma/Foveon and Sony models. In addition, MOV and related QuickTime video formats are supported such as MP4, M4V, 3G2, 3GP.",

  "packOptions": {
    "owners": [ "Drew Noakes" ],
    "projectUrl": "https://github.com/drewnoakes/metadata-extractor-dotnet",
    "licenseUrl": "http://www.apache.org/licenses/LICENSE-2.0",
    "iconUrl": "https://raw.githubusercontent.com/drewnoakes/metadata-extractor-dotnet/master/docs/metadata-extractor-logo-122.png",
    "summary": "A straightforward library for reading metadata from image and video files.",
    "requireLicenseAcceptance": false,
    "tags": [
      "Metadata",
      "Exif",
      "IPTC",
      "XMP",
      "ICC",
      "Photoshop",
      "WebP",
      "PNG",
      "BMP",
      "ICO",
      "PCX",
      "JPEG",
      "TIFF",
      "PSD",
      "Photography",
      "QuickTime",
      "MOV",
      "MP4",
      "M4V",
      "Video"
    ],
    "repository": {
      "type": "git",
      "url": "https://github.com/drewnoakes/metadata-extractor-dotnet.git"
    }
  },

  "buildOptions": {
    "debugType": "portable",
    "xmlDoc": true,
    "optimize": true,
    "nowarn": [ "1591" ],
    "keyFile": "../MetadataExtractor.snk",
    "compile": {
      "include": [ "../MetadataExtractor/**/*.cs" ],
      "exclude": [ "../MetadataExtractor/**/obj/*", "../MetadataExtractor/**/bin/*" ]
    }
  },

  "dependencies": {
    "XmpCore-Signed": "2.0.1",
    "JetBrains.Annotations": {
      "version": "10.2.1",
      "type": "build"
    }
  },

  "frameworks": {
    "netstandard1.3": {
      "dependencies": {
        "NETStandard.Library": "1.6.0",
        "System.Text.Encoding.CodePages": "4.3.0"
      }
    },
    "net35": {},
    "net45": {}
  }
}
